PORT=3000
NODE_ENV=development

# Optional: Vapi Private API Key (only needed if you want to make API calls TO Vapi)
# For webhooks that just RECEIVE data from Vapi, this is NOT required
# Get this from: https://dashboard.vapi.ai/account (Private Key, NOT Public Key)
# VAPI_API_KEY=your_vapi_private_key_here

# Optional: Webhook Secret for authenticating incoming webhook requests from Vapi
# Set this to a random string and configure the same value in Vapi dashboard headers
# This adds security to ensure only Vapi can call your webhooks
WEBHOOK_SECRET=your_webhook_secret_here
